{
  "extends": ["airbnb", "prettier", "prettier/react"],
  "plugins": ["prettier"],
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "react/jsx-uses-react": "off",
    "react/react-in-jsx-scope": "off",
    
    "prettier/prettier": ["error"],
    "linebreak-style": 0,
    
    "import/no-extraneous-dependencies": ["error", {"devDependencies": true}]
  },
  "settings": {
     "import/resolver": "webpack"
  }
}
