    "types": "dist/index.d.ts",
    "scripts": {
        "test": "jest",
        "test:generate-output": "jest --json --outputFile=.jest-test-results.json || true",
        "storybook": "start-storybook -p 9001 -c .storybook",
        "clear:storybook": "rimraf node_modules/.cache/storybook",
        "start": "npm run clear:storybook && start-storybook -p 9001",
        "build:storybook": "build-storybook -c .storybook -o ./docs --no-dll",
        "prebuild": "rm -rf dist",
        "build": "webpack",
        "postbuild": "tsc --emitDeclarationOnly true",
        "docker:build": "DOCKER_BUILDKIT=1 docker build -o dist .",
        "format": "prettier --write ./src",
        "lint": "npx eslint './src/**/*.{js,ts,tsx}' --quiet"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/equinor/fusion-components.git"
